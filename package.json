    "@nestjs/testing": "^6.2.0",
    "@nestjs/websockets": "^6.2.0",
    "mongoose": "^5.4.19",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^6.4.0",
    "swagger-ui-express": "^4.0.6",
    "typescript": "^3.2.4"
  },
  "devDependencies": {
    "@types/mongoose": "^4.7.27",
